						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        public void clicked()
        {
            super();
            UserInfo userInfo;
            DevAxRefreshDataUsersTable usersTable;
           
            ttsBegin;
            
            while select firstOnly forupdate userInfo
                join usersTable
                where usersTable.SearchUser == 0 &&
                        userInfo.networkAlias == usersTable.Email &&
                            !userInfo.enable
            {
              userInfo.enable = true;
              userInfo.update();
            }
                   

            while select forupdate userInfo
                join usersTable
                    where usersTable.SearchUser == 1 &&
                        userInfo.networkAlias like '%' + usersTable.Email + '%' 
                            && !userInfo.enable
            {
              userInfo.enable = true;
              userInfo.update();
            }
                

            ttsCommit;
        }

]]></Source>
